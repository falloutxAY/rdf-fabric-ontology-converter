        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "name",
        "displayName": "Equipment Name",
        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "manufacturer",
        "displayName": "Manufacturer",
        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "model",
        "displayName": "Model",
        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "serialNumber",
        "displayName": "Serial Number",
        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "equipmentType",
        "displayName": "Equipment Type",
        "schema": {
          "@type": "Enum",
