      {
        "@type": "Property",
        "name": "inverterId",
        "displayName": "Inverter ID",
        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "model",
        "displayName": "Model",
        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "ratedPower",
        "displayName": "Rated Power (kW)",
        "schema": "double"
      },
      {
        "@type": "Property",
        "name": "isOnline",
        "displayName": "Is Online",
