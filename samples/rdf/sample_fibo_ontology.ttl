fibo:navPerShare rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Fund ;
    rdfs:range xsd:double ;
    rdfs:label "NAV Per Share" ;
    rdfs:comment "Net Asset Value per share of the fund" .

fibo:assetsUnderManagement rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Fund ;
    rdfs:range xsd:double ;
    rdfs:label "Assets Under Management" ;
    rdfs:comment "Total value of assets managed by the fund" .

fibo:expenseRatio rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Fund ;
    rdfs:range xsd:double ;
    rdfs:label "Expense Ratio" ;
    rdfs:comment "Annual fund operating expenses as a percentage of AUM" .

