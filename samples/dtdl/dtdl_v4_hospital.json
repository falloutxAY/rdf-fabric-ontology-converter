      },
      {
        "@type": "Telemetry",
        "name": "batteryLevel",
        "displayName": "Battery Level",
        "schema": "double",
        "unit": "percent"
      },
      {
        "@type": "Telemetry",
        "name": "operatingHours",
        "displayName": "Operating Hours",
        "schema": "double"
      },
      {
        "@type": "Telemetry",
        "name": "location",
        "displayName": "Current Location",
        "schema": "string"
      },
      {
        "@type": "Command",
        "name": "runDiagnostics",
        "displayName": "Run Diagnostics"
      }
    ]
  }
]
