      },
      {
        "@type": "Telemetry",
        "name": "healthStatus",
        "displayName": "Health Status",
        "schema": "double",
        "unit": "percent"
      },
      {
        "@type": "Command",
        "name": "startCharging",
        "displayName": "Start Charging"
      },
      {
        "@type": "Command",
        "name": "stopCharging",
        "displayName": "Stop Charging"
      }
