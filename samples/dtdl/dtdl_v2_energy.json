        "displayName": "Feeds To",
        "target": "dtmi:example:energy:Battery;1"
      }
    ]
  },
  {
    "@context": "dtmi:dtdl:context;2",
    "@id": "dtmi:example:energy:Battery;1",
    "@type": "Interface",
    "displayName": "Battery",
    "description": "An energy storage battery",
    "contents": [
      {
        "@type": "Property",
        "name": "batteryId",
        "displayName": "Battery ID",
        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "capacity",
        "displayName": "Capacity (kWh)",
        "schema": "double"
      },
      {
        "@type": "Property",
        "name": "chemistry",
        "displayName": "Chemistry",
        "schema": {
          "@type": "Enum",
          "valueSchema": "string",
          "enumValues": [
            {"name": "lithiumIon", "enumValue": "lithium-ion"},
            {"name": "leadAcid", "enumValue": "lead-acid"},
            {"name": "solidState", "enumValue": "solid-state"}
          ]
        }
      },
