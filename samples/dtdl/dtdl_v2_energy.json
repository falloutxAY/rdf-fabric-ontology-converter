[
  {
    "@context": "dtmi:dtdl:context;2",
    "@id": "dtmi:example:energy:SolarPanel;1",
    "@type": "Interface",
    "displayName": "Solar Panel",
    "description": "A solar panel for energy generation",
    "contents": [
      {
        "@type": "Property",
        "name": "panelId",
        "displayName": "Panel ID",
        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "manufacturer",
        "displayName": "Manufacturer",
        "schema": "string"
      },
      {
        "@type": "Property",
        "name": "capacity",
        "displayName": "Capacity (kW)",
        "schema": "double"
      },
      {
        "@type": "Property",
