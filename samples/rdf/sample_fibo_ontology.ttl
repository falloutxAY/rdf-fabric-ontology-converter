@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix fibo: <http://example.org/fibo/> .

# =============================================================================
# Financial Industry Business Ontology (FIBO-inspired)
# This ontology models core financial concepts including parties, accounts,
# instruments, and transactions - inspired by the EDM Council's FIBO standard.
# =============================================================================

fibo:FinancialOntology rdf:type owl:Ontology ;
    rdfs:label "Financial Industry Ontology" ;
    rdfs:comment "A FIBO-inspired ontology for financial services covering parties, accounts, instruments, and transactions" .

# =============================================================================
# PARTIES - Organizations and Individuals
# =============================================================================

fibo:Party rdf:type owl:Class ;
    rdfs:label "Party" ;
    rdfs:comment "A legal entity or individual that can enter into contracts and financial arrangements" .

fibo:Organization rdf:type owl:Class ;
    rdfs:subClassOf fibo:Party ;
    rdfs:label "Organization" ;
    rdfs:comment "A formal group of people with a particular purpose, such as a business or government department" .

fibo:FinancialInstitution rdf:type owl:Class ;
    rdfs:subClassOf fibo:Organization ;
    rdfs:label "Financial Institution" ;
    rdfs:comment "An organization that provides financial services and is regulated by a financial authority" .

fibo:Bank rdf:type owl:Class ;
    rdfs:subClassOf fibo:FinancialInstitution ;
    rdfs:label "Bank" ;
    rdfs:comment "A financial institution licensed to receive deposits and make loans" .

fibo:InsuranceCompany rdf:type owl:Class ;
    rdfs:subClassOf fibo:FinancialInstitution ;
    rdfs:label "Insurance Company" ;
    rdfs:comment "A financial institution that provides risk management through insurance contracts" .

fibo:InvestmentFirm rdf:type owl:Class ;
    rdfs:subClassOf fibo:FinancialInstitution ;
    rdfs:label "Investment Firm" ;
    rdfs:comment "A financial institution that invests pooled capital on behalf of clients" .

fibo:Corporation rdf:type owl:Class ;
    rdfs:subClassOf fibo:Organization ;
    rdfs:label "Corporation" ;
    rdfs:comment "A legal entity that is separate from its owners with limited liability" .

fibo:Individual rdf:type owl:Class ;
    rdfs:subClassOf fibo:Party ;
    rdfs:label "Individual" ;
    rdfs:comment "A natural person who can be a party to financial transactions" .

fibo:Customer rdf:type owl:Class ;
    rdfs:subClassOf fibo:Party ;
    rdfs:label "Customer" ;
    rdfs:comment "A party that has a business relationship with a financial institution" .

# =============================================================================
# ACCOUNTS - Financial Accounts
# =============================================================================

fibo:Account rdf:type owl:Class ;
    rdfs:label "Account" ;
    rdfs:comment "A record of financial transactions and balances maintained by a financial institution" .

fibo:DepositAccount rdf:type owl:Class ;
    rdfs:subClassOf fibo:Account ;
    rdfs:label "Deposit Account" ;
    rdfs:comment "An account that holds funds deposited by an account holder" .

fibo:CheckingAccount rdf:type owl:Class ;
    rdfs:subClassOf fibo:DepositAccount ;
    rdfs:label "Checking Account" ;
    rdfs:comment "A transactional deposit account for day-to-day banking needs" .

fibo:SavingsAccount rdf:type owl:Class ;
    rdfs:subClassOf fibo:DepositAccount ;
    rdfs:label "Savings Account" ;
    rdfs:comment "A deposit account that earns interest on deposited funds" .

fibo:LoanAccount rdf:type owl:Class ;
    rdfs:subClassOf fibo:Account ;
    rdfs:label "Loan Account" ;
    rdfs:comment "An account that tracks a loan extended to a borrower" .

fibo:MortgageAccount rdf:type owl:Class ;
    rdfs:subClassOf fibo:LoanAccount ;
    rdfs:label "Mortgage Account" ;
    rdfs:comment "A loan account secured by real property" .

fibo:CreditCardAccount rdf:type owl:Class ;
    rdfs:subClassOf fibo:LoanAccount ;
    rdfs:label "Credit Card Account" ;
    rdfs:comment "A revolving credit account accessed via a credit card" .

fibo:InvestmentAccount rdf:type owl:Class ;
    rdfs:subClassOf fibo:Account ;
    rdfs:label "Investment Account" ;
    rdfs:comment "An account used to hold and trade financial instruments" .

fibo:BrokerageAccount rdf:type owl:Class ;
    rdfs:subClassOf fibo:InvestmentAccount ;
    rdfs:label "Brokerage Account" ;
    rdfs:comment "An investment account held at a registered brokerage firm" .

fibo:RetirementAccount rdf:type owl:Class ;
    rdfs:subClassOf fibo:InvestmentAccount ;
    rdfs:label "Retirement Account" ;
    rdfs:comment "A tax-advantaged investment account for retirement savings" .

# =============================================================================
# FINANCIAL INSTRUMENTS - Securities and Derivatives
# =============================================================================

fibo:FinancialInstrument rdf:type owl:Class ;
    rdfs:label "Financial Instrument" ;
    rdfs:comment "A tradeable asset or a contractual right to receive or deliver assets" .

fibo:Security rdf:type owl:Class ;
    rdfs:subClassOf fibo:FinancialInstrument ;
    rdfs:label "Security" ;
    rdfs:comment "A tradable financial instrument that represents ownership or debt" .

fibo:EquitySecurity rdf:type owl:Class ;
    rdfs:subClassOf fibo:Security ;
    rdfs:label "Equity Security" ;
    rdfs:comment "A security representing ownership interest in an entity" .

fibo:CommonStock rdf:type owl:Class ;
    rdfs:subClassOf fibo:EquitySecurity ;
    rdfs:label "Common Stock" ;
    rdfs:comment "Equity security representing voting shares in a corporation" .

fibo:PreferredStock rdf:type owl:Class ;
    rdfs:subClassOf fibo:EquitySecurity ;
    rdfs:label "Preferred Stock" ;
    rdfs:comment "Equity security with preferential dividend rights but typically no voting rights" .

fibo:DebtSecurity rdf:type owl:Class ;
    rdfs:subClassOf fibo:Security ;
    rdfs:label "Debt Security" ;
    rdfs:comment "A security representing a creditor relationship with an entity" .

fibo:Bond rdf:type owl:Class ;
    rdfs:subClassOf fibo:DebtSecurity ;
    rdfs:label "Bond" ;
    rdfs:comment "A fixed income instrument representing a loan made by an investor to a borrower" .

fibo:GovernmentBond rdf:type owl:Class ;
    rdfs:subClassOf fibo:Bond ;
    rdfs:label "Government Bond" ;
    rdfs:comment "A bond issued by a national government" .

fibo:CorporateBond rdf:type owl:Class ;
    rdfs:subClassOf fibo:Bond ;
    rdfs:label "Corporate Bond" ;
    rdfs:comment "A bond issued by a corporation to fund operations or expansion" .

fibo:MunicipalBond rdf:type owl:Class ;
    rdfs:subClassOf fibo:Bond ;
    rdfs:label "Municipal Bond" ;
    rdfs:comment "A bond issued by a state, city, or county government" .

fibo:Derivative rdf:type owl:Class ;
    rdfs:subClassOf fibo:FinancialInstrument ;
    rdfs:label "Derivative" ;
    rdfs:comment "A financial instrument whose value is derived from an underlying asset" .

fibo:Option rdf:type owl:Class ;
    rdfs:subClassOf fibo:Derivative ;
    rdfs:label "Option" ;
    rdfs:comment "A contract giving the right but not obligation to buy or sell an asset" .

fibo:Future rdf:type owl:Class ;
    rdfs:subClassOf fibo:Derivative ;
    rdfs:label "Future" ;
    rdfs:comment "A standardized contract to buy or sell an asset at a future date" .

fibo:Swap rdf:type owl:Class ;
    rdfs:subClassOf fibo:Derivative ;
    rdfs:label "Swap" ;
    rdfs:comment "A derivative contract to exchange cash flows between two parties" .

fibo:Fund rdf:type owl:Class ;
    rdfs:subClassOf fibo:FinancialInstrument ;
    rdfs:label "Fund" ;
    rdfs:comment "A pooled investment vehicle managed by an investment company" .

fibo:MutualFund rdf:type owl:Class ;
    rdfs:subClassOf fibo:Fund ;
    rdfs:label "Mutual Fund" ;
    rdfs:comment "An open-ended investment fund that pools money from many investors" .

fibo:ETF rdf:type owl:Class ;
    rdfs:subClassOf fibo:Fund ;
    rdfs:label "Exchange Traded Fund" ;
    rdfs:comment "A fund that trades on stock exchanges like individual stocks" .

fibo:HedgeFund rdf:type owl:Class ;
    rdfs:subClassOf fibo:Fund ;
    rdfs:label "Hedge Fund" ;
    rdfs:comment "An alternative investment fund using pooled funds with different strategies" .

# =============================================================================
# TRANSACTIONS - Financial Operations
# =============================================================================

fibo:Transaction rdf:type owl:Class ;
    rdfs:label "Transaction" ;
    rdfs:comment "A financial operation that changes the state of an account or transfers value" .

fibo:Payment rdf:type owl:Class ;
    rdfs:subClassOf fibo:Transaction ;
    rdfs:label "Payment" ;
    rdfs:comment "A transaction involving the transfer of money from one party to another" .

fibo:Transfer rdf:type owl:Class ;
    rdfs:subClassOf fibo:Transaction ;
    rdfs:label "Transfer" ;
    rdfs:comment "A movement of funds between accounts" .

fibo:Trade rdf:type owl:Class ;
    rdfs:subClassOf fibo:Transaction ;
    rdfs:label "Trade" ;
    rdfs:comment "A transaction involving the exchange of financial instruments" .

fibo:BuyOrder rdf:type owl:Class ;
    rdfs:subClassOf fibo:Trade ;
    rdfs:label "Buy Order" ;
    rdfs:comment "An order to purchase a financial instrument" .

fibo:SellOrder rdf:type owl:Class ;
    rdfs:subClassOf fibo:Trade ;
    rdfs:label "Sell Order" ;
    rdfs:comment "An order to sell a financial instrument" .

fibo:Deposit rdf:type owl:Class ;
    rdfs:subClassOf fibo:Transaction ;
    rdfs:label "Deposit" ;
    rdfs:comment "A transaction adding funds to an account" .

fibo:Withdrawal rdf:type owl:Class ;
    rdfs:subClassOf fibo:Transaction ;
    rdfs:label "Withdrawal" ;
    rdfs:comment "A transaction removing funds from an account" .

fibo:DividendPayment rdf:type owl:Class ;
    rdfs:subClassOf fibo:Payment ;
    rdfs:label "Dividend Payment" ;
    rdfs:comment "A payment of earnings distributed to shareholders" .

fibo:InterestPayment rdf:type owl:Class ;
    rdfs:subClassOf fibo:Payment ;
    rdfs:label "Interest Payment" ;
    rdfs:comment "A payment for the use of borrowed money" .

# =============================================================================
# CONTRACTS AND AGREEMENTS
# =============================================================================

fibo:Contract rdf:type owl:Class ;
    rdfs:label "Contract" ;
    rdfs:comment "A legally binding agreement between two or more parties" .

fibo:LoanAgreement rdf:type owl:Class ;
    rdfs:subClassOf fibo:Contract ;
    rdfs:label "Loan Agreement" ;
    rdfs:comment "A contract for the lending of money with terms for repayment" .

fibo:InsurancePolicy rdf:type owl:Class ;
    rdfs:subClassOf fibo:Contract ;
    rdfs:label "Insurance Policy" ;
    rdfs:comment "A contract providing financial protection against specified risks" .

fibo:ServiceAgreement rdf:type owl:Class ;
    rdfs:subClassOf fibo:Contract ;
    rdfs:label "Service Agreement" ;
    rdfs:comment "A contract for the provision of financial services" .

# =============================================================================
# REGULATORY AND COMPLIANCE
# =============================================================================

fibo:RegulatoryAuthority rdf:type owl:Class ;
    rdfs:subClassOf fibo:Organization ;
    rdfs:label "Regulatory Authority" ;
    rdfs:comment "A government body responsible for regulating financial activities" .

fibo:Jurisdiction rdf:type owl:Class ;
    rdfs:label "Jurisdiction" ;
    rdfs:comment "A geographical area with its own set of laws and regulations" .

fibo:ComplianceRequirement rdf:type owl:Class ;
    rdfs:label "Compliance Requirement" ;
    rdfs:comment "A regulatory requirement that must be met by financial institutions" .

# =============================================================================
# RISK MANAGEMENT
# =============================================================================

fibo:Risk rdf:type owl:Class ;
    rdfs:label "Risk" ;
    rdfs:comment "The potential for financial loss or adverse outcome" .

fibo:CreditRisk rdf:type owl:Class ;
    rdfs:subClassOf fibo:Risk ;
    rdfs:label "Credit Risk" ;
    rdfs:comment "Risk of loss due to a borrower's failure to repay a loan" .

fibo:MarketRisk rdf:type owl:Class ;
    rdfs:subClassOf fibo:Risk ;
    rdfs:label "Market Risk" ;
    rdfs:comment "Risk of losses due to changes in market prices" .

fibo:OperationalRisk rdf:type owl:Class ;
    rdfs:subClassOf fibo:Risk ;
    rdfs:label "Operational Risk" ;
    rdfs:comment "Risk of loss from inadequate or failed processes, people, or systems" .

fibo:LiquidityRisk rdf:type owl:Class ;
    rdfs:subClassOf fibo:Risk ;
    rdfs:label "Liquidity Risk" ;
    rdfs:comment "Risk of being unable to meet short-term financial obligations" .

# =============================================================================
# DATA PROPERTIES - Party Properties
# =============================================================================

fibo:partyId rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Party ;
    rdfs:range xsd:string ;
    rdfs:label "Party ID" ;
    rdfs:comment "Unique identifier for the party" .

fibo:partyName rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Party ;
    rdfs:range xsd:string ;
    rdfs:label "Party Name" ;
    rdfs:comment "Legal name of the party" .

fibo:legalEntityIdentifier rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Organization ;
    rdfs:range xsd:string ;
    rdfs:label "Legal Entity Identifier" ;
    rdfs:comment "LEI code for the organization" .

fibo:taxIdentificationNumber rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Party ;
    rdfs:range xsd:string ;
    rdfs:label "Tax Identification Number" ;
    rdfs:comment "Tax ID for the party" .

fibo:incorporationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Organization ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Incorporation Date" ;
    rdfs:comment "Date when the organization was legally formed" .

fibo:dateOfBirth rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Individual ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Date of Birth" ;
    rdfs:comment "Birth date of the individual" .

fibo:nationality rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Individual ;
    rdfs:range xsd:string ;
    rdfs:label "Nationality" ;
    rdfs:comment "Country of citizenship" .

fibo:creditScore rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Party ;
    rdfs:range xsd:integer ;
    rdfs:label "Credit Score" ;
    rdfs:comment "Numerical credit rating of the party" .

fibo:riskRating rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Party ;
    rdfs:range xsd:string ;
    rdfs:label "Risk Rating" ;
    rdfs:comment "Risk classification assigned to the party" .

fibo:kycStatus rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Customer ;
    rdfs:range xsd:string ;
    rdfs:label "KYC Status" ;
    rdfs:comment "Know Your Customer verification status" .

fibo:customerSince rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Customer ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Customer Since" ;
    rdfs:comment "Date when the customer relationship began" .

# =============================================================================
# DATA PROPERTIES - Account Properties
# =============================================================================

fibo:accountNumber rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Account ;
    rdfs:range xsd:string ;
    rdfs:label "Account Number" ;
    rdfs:comment "Unique identifier for the account" .

fibo:accountName rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Account ;
    rdfs:range xsd:string ;
    rdfs:label "Account Name" ;
    rdfs:comment "Display name for the account" .

fibo:accountStatus rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Account ;
    rdfs:range xsd:string ;
    rdfs:label "Account Status" ;
    rdfs:comment "Current status of the account (Active, Closed, Frozen, etc.)" .

fibo:openDate rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Account ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Open Date" ;
    rdfs:comment "Date when the account was opened" .

fibo:closeDate rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Account ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Close Date" ;
    rdfs:comment "Date when the account was closed" .

fibo:currentBalance rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Account ;
    rdfs:range xsd:double ;
    rdfs:label "Current Balance" ;
    rdfs:comment "Current balance of the account" .

fibo:availableBalance rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Account ;
    rdfs:range xsd:double ;
    rdfs:label "Available Balance" ;
    rdfs:comment "Available balance for transactions" .

fibo:currency rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Account ;
    rdfs:range xsd:string ;
    rdfs:label "Currency" ;
    rdfs:comment "ISO currency code for the account" .

fibo:interestRate rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:Account ;
    rdfs:range xsd:double ;
    rdfs:label "Interest Rate" ;
    rdfs:comment "Annual interest rate as a percentage" .

fibo:creditLimit rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:CreditCardAccount ;
    rdfs:range xsd:double ;
    rdfs:label "Credit Limit" ;
    rdfs:comment "Maximum credit available on the account" .

fibo:minimumPayment rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:LoanAccount ;
    rdfs:range xsd:double ;
    rdfs:label "Minimum Payment" ;
    rdfs:comment "Minimum required periodic payment" .

fibo:principalAmount rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:LoanAccount ;
    rdfs:range xsd:double ;
    rdfs:label "Principal Amount" ;
    rdfs:comment "Original loan amount" .

fibo:maturityDate rdf:type owl:DatatypeProperty ;
    rdfs:domain fibo:LoanAccount ;
    rdfs:range xsd:dateTime ;
    rdfs:label "Maturity Date" ;
    rdfs:comment "Date when the loan is due to be fully repaid" .

